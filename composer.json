{
    "name": "r2h/laravel-stubs",
    "description": "Default Laravel stubs to our liking.",
    "homepage": "https://github.com/R2H-BV/laravel-stubs",
    "type": "laravel-plugin",
    "keywords": [
        "r2h",
        "laravel",
        "stubs"
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "R2H\\Stubs\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "R2H\\Stubs\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Michael Snoeren",
            "email": "michael@r2h.nl",
            "homepage": "https://r2h.nl",
            "role": "Developer"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.0",
        "laravel/framework": "^8.0|^9.0|^10.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "R2H\\Stubs\\StubsServiceProvider"
            ]
        }
    },
    "require-dev": {
        "orchestra/testbench": "^8.0",
        "phpunit/phpunit": "^10.0",
        "r2h/r2h-coding-standard": "^v2.3.1"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
